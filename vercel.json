{
  "version": 2,
  "name": "proxywhirl-docs",
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "buildCommand": "cd docs && pnpm build",
  "outputDirectory": "docs/out",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "functions": {
    "docs/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}